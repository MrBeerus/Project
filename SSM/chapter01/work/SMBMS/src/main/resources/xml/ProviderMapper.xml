<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beerus.dao.mapper.ProviderMapper">
    <!--MyBatis映射文件-->
    <select id="count_Prov" resultType="int">
        SELECT COUNT(1) AS row_count FROM smbms_provider;
    </select>
    <!--查询供应商表所有数据-->
    <select id="list_AllProv" resultType="SmbmsProvider">
        SELECT * FROM smbms_provider;
    </select>
</mapper>